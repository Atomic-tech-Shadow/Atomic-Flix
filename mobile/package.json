{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "doctor": "node doctor-check.js",
    "check-deps": "expo install --check",
    "fix-deps": "expo install --fix",
    "health": "expo doctor && npm audit --audit-level=moderate",
    "termux:setup": "./termux-setup.sh",
    "termux:validate": "./termux-validate.sh",
    "termux:build": "./termux-build-apk.sh",
    "build:android": "eas build --platform android",
    "build:production": "eas build --platform android --profile production",
    "clean": "expo r -c"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@egjs/hammerjs": "^2.0.17",
    "@react-navigation/native": "^6.0.0",
    "@react-navigation/stack": "^6.0.0",
    "@tanstack/react-query": "^5.82.0",
    "@types/hammerjs": "^2.0.46",
    "expo": "~53.0.19",
    "expo-build-properties": "^0.14.8",
    "expo-status-bar": "~2.2.3",
    "hoist-non-react-statics": "^3.3.2",
    "react": "19.0.0",
    "react-freeze": "^1.0.4",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.11.1",
    "expo-linear-gradient": "~14.1.5",
    "babel-preset-expo": "~13.0.0",
    "@babel/core": "^7.20.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.20.0",
    "@types/react": "~19.0.10",
    "@types/react-native": "~0.79.5",
    "typescript": "~5.8.3",
    "@expo/cli": "^0.22.4"
  },
  "overrides": {
    "@types/react": "~19.0.10"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "listUnknownPackages": false,
        "exclude": []
      },
      "appConfigFieldsNotSyncedCheck": {
        "enabled": true
      }
    }
  }
}