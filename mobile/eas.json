{
  "cli": {
    "version": ">= 5.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease --no-daemon -x checkReleaseAarMetadata -x lintVitalAnalyzeRelease"
      },
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_HERMES": "false"
      },
      "cache": {
        "disabled": true
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "env": {
          "ANDROID_COMPILE_SDK": "34",
          "ANDROID_BUILD_TOOLS": "34.0.0",
          "ANDROID_MIN_SDK": "21",
          "ANDROID_TARGET_SDK": "34",
          "EXPO_USE_HERMES": "false"
        }
      },
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
}